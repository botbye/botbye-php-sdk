{
    "name": "botbye/botbye-php-sdk",
    "description": "Botbye PHP SDK for bot detection and account takeover protection",
    "type": "library",
    "license": "MIT",
    "keywords": ["botbye", "bot-detection", "security", "ato-protection"],
    "homepage": "https://github.com/botbye/botbye-php-sdk",
    "authors": [
        {
            "name": "Botbye",
            "email": "accounts@botbye.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/http-client": "^6.0|^7.0",
        "psr/log": "^3.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "symfony/var-dumper": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Botbye\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Botbye\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}